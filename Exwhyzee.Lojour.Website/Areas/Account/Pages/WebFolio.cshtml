<body class="home page page-id-18 page-template page-template-homepage page-template-homepage-php intro-welcome" style="display: block;">
    <div id="awe-language">
    </div>
    <!--=============== main ===============-->
    <div id="main" style="opacity: 1;">

        <!--=============== intro ===============-->
        <div id="intro">

            <!--=============== navigation ===============-->

            <div class="nav-holder">

                <div class="nav-button transition">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>

                <div class="nav-border fade" style="opacity: 1;"></div>
                <div class="link-holder fade" style="opacity: 1;">
                    <div class="main-menu-wrapper">
                        <div class="menu-chulan-menu-container">
                            <ul id="menu-chulan-menu" class="navbar-nav nav">
                                <li class="transition"><a href="#about" data-scroll="" class="transition">About</a></li>
                                <li class="transition"><a href="#resume" data-scroll="" class="transition">Resume</a></li>
                                <li class="transition"><a href="#work" data-scroll="" class="transition">Portfolio</a></li>
                                <li class="transition"><a href="#services" data-scroll="" class="transition">Services</a></li>
                                <li class="transition"><a href="#contacts" data-scroll="" class="transition">Contacts</a></li>
                                <li class="transition"><a href="chulan-blog/" class="transition">Blog</a></li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div><!--=============== navigation end ===============-->
            <!--=============== text-ticker-holder ===============-->

            <div class="text-ticker-holder">
                <div class="overlay"></div>
                <div class="container">

                    <div class="anim-text-holder">

                        <h1 class="animtext intro-title-home" style="font-size: 66.6667px;">
                            <ul class="texts" style="display: none;"><li class="current">Hi I am @Model.userProfileModel.SurName @Model.userProfileModel.FirstName @Model.userProfileModel.LastName</li></ul>
                        </h1>
                        <h3 class="animtext intro-subtitle-home fade" style="opacity: 1;color:#000000;font-weight:900;">@Model.userProfileModel.Title</h3>

                        <div class="start-holder fade" style="opacity: 1;">

                            <div class="start-decor transition"></div>

                            <a href="#about" class="scroll-link start"><i class="fa fa-angle-down"></i></a>

                        </div>

                    </div>

                </div>

            </div>	<!--=============== text-ticker-holder end===============-->

        </div><!--=============== intro end ===============-->
        <!--=============== wrapper ===============-->

        <div id="wrapper">

            <!--=============== left-sidebar ===============-->

            <div class="left-sidebar">
                <div class="logo-wrapper">
                    <a href="/" class="scroll-link logo">
                    <img src="~/img/lojour-logo-web.png" alt=""></a>
                    <p class="slogan">Touching in the moment</p>
                </div>
                <div class="desktop-screen-navigation" id="nav">
                    <div class="menu-chulan-menu-container">
                        <ul id="menu-chulan-menu-1" class="navbar-nav nav">
                            <li class="transition"><a href="#about" data-scroll="" class="transition">About</a></li>
                            <li class="transition"><a href="#resume" data-scroll="" class="transition">Resume</a></li>
                            <li class="transition"><a href="#work" data-scroll="" class="transition">Portfolio</a></li>
                            <li class="transition"><a href="#contacts" data-scroll="" class="transition">Contacts</a></li>
                        </ul>
                    </div>
                </div>
                <a data-scroll="" class="nav-top" href="#intro"><i class="fa fa-angle-up fa-2x"></i><span class="transition2"></span></a>
            </div>
            <!--=============== left-sidebar end ===============-->
            <!--=============== content-holder ===============-->

            <div class="content-holder">
                <div id="content-sort">
                    <!--=============== section about ===============-->
                    <div id="about" class="sections">

                        <div class="container animaper anim" style="opacity: 1; top: 0px;">

                            <h2>ABOUT</h2>
                            <h4 class="margin js-subtitle">A small introduction about my self</h4>


                            <div class="grid-half right">

                                <div class="photo-holder">

                                    <img src="@Model.userProfileModel.PhotoUrl" alt="" style="max-height:300px;" class="respimg">

                                </div>

                            </div>

                            <div class="grid-half right">

                                <h3>@Model.userProfileModel.SurName @Model.userProfileModel.FirstName @Model.userProfileModel.LastName</h3>
                                <h3 class="margin">@Model.userProfileModel.Title</h3>
                                <p>
                                    @Model.userProfileModel.Description
                                </p>
                                <div class="clear"></div>
                                <a href="#work" class="button scroll-link transition">view my work</a>
                            </div>

                            <div class="clear"></div>

                            <!--skills-->

                        </div>

                        <div class="show-skills-holder">
                            <span class="decor-fix"></span>
                            <div class="show-skills"><i class="fa fa-angle-up fa-2x transition"></i><span>show skills</span></div>
                        </div>

                    </div><!--=============== section about end ===============-->
                    <!--=============== section facts ===============-->

                    <div id="facts" class="sections">
                        <div class="parallax-facts"></div>
                        <div class="overlay"></div>

                        <div class="content">

                            <div class="container">
                                <h4 class="margin">ABOUT ANYTHING</h4>
                                <h2 class="color-white">FUN FACTS</h2>

                                <ul>
                                    @foreach (var item in Model.ListJobAnalysisModel)
                                    {
                                        <li class="transition animaper">
                                            <div class="conter-decor">
                                                <span class="rotade rot-top-left"></span>
                                                <span class="rotade rot-top-right"></span>
                                                <span class="rotade rot-bottom-left"></span>
                                                <span class="rotade rot-bottom-right"></span>
                                            </div>
                                            <div class="milestone-counter">

                                                <div class="stats animaper">
                                                    <div class="num" data-content="@item.Count" data-num="@item.Count">@item.Count</div>
                                                </div>

                                            </div>

                                            <h6><span></span>@item.Title</h6>

                                        </li>
                                    }


                                </ul>

                            </div>

                        </div>

                    </div> <!--=============== section facts end ===============-->
                    <!--=============== section resume ===============-->

                    <div id="resume" class="sections">
                        <div class="background-color">
                            <div class="content">
                                <div class="container animaper anim" style="opacity: 1; top: 0px;">

                                    <h4 class="margin color-white">WORK EXPERIENCE</h4>
                                    <h2 class="color-white">RESUME</h2>
                                    <div class="content-separator"><i class="fa fa-briefcase"></i><span></span></div>
                                    <div class="grid-full">
                                        <div class="resume-line"></div>
                                        <!-- 1 item-->
                                        @foreach (var item in Model.ListExperienceModel)
                                        {
                                            <div class="grid-full place-holder">
                                                <div class="grid-1">

                                                    <div class="resume-date">
                                                        <h4>@item.StartDate-@item.EndDate</h4>
                                                        <span>@item.ExperienceType</span>
                                                    </div>

                                                </div>

                                                <div class="grid-5 right">

                                                    <div class="resume-info">

                                                        <h3>@item.Title</h3>
                                                        <h4>@item.Address</h4>
                                                        <p>
                                                            @item.Description
                                                        </p>

                                                    </div>

                                                </div>

                                            </div>
                                        }
                                        <!-- 1 item-->

                                    <a asp-area="Account" asp-page="/MyCv" class="button transition"><i class="fa fa-angle-down"></i> Download resume</a>
                                    </div>
                                </div>

                            </div>

                        </div>

                        <div class="resumeshadow"></div>

                    </div><!--=============== section resume end===============-->
                    <!--=============== section work ===============-->

                    <div id="work" class="sections">

                        <div class="background-white">

                            <div class="content">

                                <div class="container animaper anim" style="opacity: 1; top: 0px;">

                                    <h2>WORK</h2>
                                    <h4 class="margin">PORTFOLIO</h4>

                                    <!-- portfolio filters -->

                                    <div id="options" class="clear">

                                        <ul id="filters" class="option-set" data-option-key="filter">


                                            <li>Pictures of Active and Completed Jobs</li>



                                        </ul>

                                    </div>


                                    <!-- Projects links -->

                                    <div id="folio_container">
                                        @foreach (var item in Model.ListWorkImageModel)
                                        {


                                            <div class="box grid-2 mix branding photo web mix_all" style="display: inline-block;  opacity: 1;">
                                                <a href="http://localhost:49784/@item.Url" data-url="http://localhost:49784/@item.Url" class="popup-with-move-anim">

                                                    <div class="folio-img-holder">
                                                        <img src="@item.Url" class="respimg" alt="" title="" style="max-height:400px;">
                                                        <div class="folio-overlay">
                                                            <div class="is-table">
                                                                <div class="table-cell">
                                                                    <div class="overlay-content">
                                                                        <h3>@item.Title</h3>
                                                                        <h5>@item.Address</h5>
                                                                        <div class="folio-button"><i class="fa fa-rocket"></i></div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </a>

                                            </div>
                                        }

                                    </div>

                                </div>

                            </div>

                        </div>

                        <div class="workshadow"></div>

                    </div>	<!--=============== section work  end===============-->



                <div id="footer" class="sections ws">
                    <div class="content bs">
                        <div class="container">

                            <a class="to-top scroll-link" href="#intro"><i class="fa fa-angle-up transition"></i></a>

                            <div class="clear"></div>

                            <div class="social-list">
                                <ul>
                                    <li>
                                        <a href="@Model.userProfileModel.FacebookLink" target="_blank" class="transition">
                                            <i class="fa fa-facebook"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Model.userProfileModel.LinkedinLink" target="_blank" class="transition"><i class="fa fa-linkedin"></i></a>
                                    </li>
                                    <li>
                                        <a href="@Model.userProfileModel.TwitterLink" target="_blank" class="transition"><i class="fa fa-twitter"></i></a>
                                    </li>
                                </ul>
                            </div>           <p class="copyright">Copyright @DateTime.UtcNow.Year. A product of Exwhyzee Technologies</p>

                            <p class="site-info">
                                <a href="http://lojour.com/">Proudly powered by Lojour.com</a>
                            </p><!-- .site-info -->

                        </div>

                    </div>

                </div><!--=============== section footer end ===============-->

            </div><!--=============== content-holder end ===============-->

        </div><!--=============== wrapper end ===============-->


    </div><!--=============== main end ===============-->

    <script type="text/javascript" src="~/folio/jquery.form.min.js"></script>

    <script type="text/javascript" src="~/folio/scripts.js"></script>
    <script type="text/javascript" src="~/folio/core.min.js"></script>
    <script type="text/javascript" src="~/folio/widget.min.js"></script>
    <script type="text/javascript" src="~/folio/tabs.min.js"></script>
    <script type="text/javascript" src="~/folio/accordion.min.js"></script>
    <script type="text/javascript" src="~/folio/js"></script>
    <script type="text/javascript" src="~/folio/jpreloader.min.js"></script>
    <script type="text/javascript" src="~/folio/smoothscroll.js"></script>
    <script type="text/javascript" src="~/folio/jquery.scrollTo-1.4.2-min.js"></script>
    <script type="text/javascript" src="~/folio/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="~/folio/jquery.lettering.js"></script>
    <script type="text/javascript" src="~/folio/jquery.textillate.js"></script>
    <script type="text/javascript" src="~/folio/jquery.fittext.js"></script>
    <script type="text/javascript" src="~/folio/mixitup.js"></script>
    <script type="text/javascript" src="~/folio/appear.js"></script>
    <script type="text/javascript" src="~/folio/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="~/folio/jquery.parallax-1.1.3.js"></script>
    <script type="text/javascript" src="~/folio/jquery.easypiechart.js"></script>
    <script type="text/javascript" src="~/folio/jquery.nav.js"></script>
    <script type="text/javascript" src="~/folio/jquery.home.js"></script>
    <script type="text/javascript" src="~/folio/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/folio/static-scripts.js"></script>
    <script type="text/javascript" src="~/folio/initChulan.js"></script>

    <script>

    </script>
</body>
