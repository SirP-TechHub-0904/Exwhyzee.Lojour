@page
@model Exwhyzee.Lojour.Website.Areas.Account.Pages.UpdateProfileModel
@{
    Layout = "/Views/Shared/_ClientLayout.cshtml";

}

<style>
    .checkbox label, .radio label {
        padding-left: 0px !important;
    }
</style>
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3>Update Profile</h3>
        </div>

    </div>
    <div class="clearfix"></div>
    <div class="row">

        <div class="col-md-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">


                    <h2>
                        Profile
                        <a class="collapse-link pull-right"><i class="fa fa-chevron-up">min/max</i></a>
                    </h2>

                    <div class="clearfix"></div>
                </div>
                <div class="x_content">

                    <div class=" profile_left">
                        <div class="profile_img">
                            <div id="crop-avatar">
                                <!-- Current avatar -->
                                <img class="img-responsive avatar-view" src="@Model.userProfileModel.PhotoUrl" alt="Avatar" title="" style="max-height:100px;">
                            </div>
                        </div>

                        <h3>@Model.userProfileModel.SurName @Model.userProfileModel.FirstName @Model.userProfileModel.LastName </h3>

                        <ul class="list-unstyled user_data">
                            <li>
                                <i class="fa fa-file user-profile-icon"></i> @Model.userProfileModel.Title
                            </li>
                            <li>
                                <i class="fa fa-map-marker user-profile-icon"></i> @Model.userProfileModel.ContactAddress, @Model.userProfileModel.Country
                            </li>

                            @*<li>
            <i class="fa fa-briefcase user-profile-icon"></i> Occupation
        </li>*@

                            <li class="m-top-xs">
                                <i class="fa fa-external-link user-profile-icon"></i>
                                <a href="http://www.lojour.com/" target="_blank">www.lojour.com</a>
                            </li>
                        </ul>



                    </div>
                </div>

                <div class="x_content">


                    <form class="form-horizontal form-label-left" asp-page-handler="UserProfile" enctype="multipart/form-data" method="post">
                        <input type="hidden" asp-for="userProfileModel.DateRegistered" name="userProfileModel.DateRegistered" />
                        <input type="hidden" asp-for="userProfileModel.UserId" name="userProfileModel.UserId" />
                        <input type="hidden" asp-for="userProfileModel.Id" name="userProfileModel.Id" />
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="control-label">
                                        Job Title
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.Title" name="userProfileModel.Title" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        Surname
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.SurName" name="userProfileModel.SurName" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        First Name
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.FirstName" name="userProfileModel.FirstName" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        Last Name
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.LastName" name="userProfileModel.LastName" class="form-control">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Gender
                                        </label>
                                        <div class="">
                                            <select class="form-control" asp-for="userProfileModel.Gender" name="userProfileModel.Gender">
                                                <option value="">Choose</option>
                                                <option value="Male">Male</option>
                                                <option value="Female">Female</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            MaritalStatus
                                        </label>
                                        <div class="">
                                            <select class="form-control" asp-for="userProfileModel.MaritalStatus" name="userProfileModel.MaritalStatus">
                                                <option value="">Choose</option>
                                                <option value="Single">Single</option>
                                                <option value="Married">Married</option>
                                                <option value="Widow">Widow</option>
                                                <option value="Widower">Widower</option>
                                                <option value="Divorced">Divorced</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="item form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Address
                                        </label>
                                        <div class="">
                                            <textarea class="form-control" asp-for="userProfileModel.ContactAddress" name="userProfileModel.ContactAddress"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Description
                                        </label>
                                        <div class="">
                                            <textarea class="form-control" asp-for="userProfileModel.Description" name="userProfileModel.Description"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item form-group">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Date
                                        </label>
                                        <div class="">
                                            <input type="date" asp-for="userProfileModel.DateOfBirth" name="userProfileModel.DateOfBirth" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Country
                                        </label>
                                        <div class="">
                                            <input type="text" asp-for="userProfileModel.Country" name="userProfileModel.Country" class="form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        Facebook Link
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.FacebookLink" name="userProfileModel.FacebookLink" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        Twitter Link
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.TwitterLink" name="userProfileModel.TwitterLink" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        Linkedin Link
                                    </label>
                                    <div class="">
                                        <input type="text" asp-for="userProfileModel.LinkedinLink" name="userProfileModel.LinkedinLink" class="form-control">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label">
                                Complementry Card Keywords
                            </label>
                            <div class="">
                                <input type="text" asp-for="userProfileModel.ComplementryCardKeywords" name="userProfileModel.ComplementryCardKeywords" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label">
                                Profile Photo
                            </label>
                            <div class="">
                                <input class="form-control" type="file" name="files" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="">

                                <button id="submit" type="submit" class="btn btn-success">Update</button>
                            </div>
                        </div>


                    </form>
                </div>
            </div>
        </div>




        <div class="col-md-6 col-sm-6 col-xs-12">
            <div class="x_panel">
                <div class="x_title">


