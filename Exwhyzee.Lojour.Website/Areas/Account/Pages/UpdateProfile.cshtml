                            <form asp-page-handler="ExperienceDelete" asp-route-id="@item.Id" method="post">
                                <button class="pull-right" style="font-weight:900;">X</button>
                            </form>
                           
                                <p>
                                    Title: @item.Title
                                    @if (item.IsCurrent == true)
                                    {
                                        <span class="btn-sm btn-success">
                                            Is Current
                                        </span>
                                    }
                                    else
                                    {


                                    }



                                </p>

                                <p>Address: @item.Address</p>

                                <p>Start - End Date: @item.StartDate - @item.EndDate</p>

                                <p>Description: @item.Description</p>

                            
                        </div>


                        }

                    
